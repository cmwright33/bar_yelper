<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="navbar-inner">
    <div class="container">
          <ul class="nav">
            <li class="brand">Barmapper</li>
              <% if current_user.nil? %>
                <li><%= link_to "Sign up", sign_up_path %> or </li>
                <li><%= link_to "Log in", log_in_path %></li>
              <% else %>
                <li><%= link_to "Neighborhoods", neighborhoods_path %></li>
                <li><%= link_to "Favorite Bars", favoritebar_index_path %> </li>
                <li><%= link_to "Users", users_path%></li>
              <% end %>
              <% if current_user %>
                  <li>Logged in as <%= current_user.name %>.</li>
                  <li><%= link_to "Log out", log_out_path %></li>

              <% end %>
            </ul>
      </nav>
    </div>
  </div>
</header>